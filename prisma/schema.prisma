datasource db {
  provider = "sqlite"
}

generator client {
  provider      = "prisma-client"
  output        = "../src/prisma/generated"
  moduleFormat  = "cjs"
}

model Room {
  id          Int       @default(autoincrement()) @id
  slug        String    @unique
  strokes     Json      @default("[]")
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}